{
  "expo": {
    "name": "UniCar",
    "slug": "UniCarApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "unicarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/icons/ios-icon-dark.svg",
        "light": "./assets/icons/ios-icon-light.svg"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.weslleysantiago.UniCarApp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#fff",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#003638"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "368935fd-4fd3-421b-9cd4-6b0ddcfe8bf0"
      }
    },
    "owner": "weslleysantiago"
  }
}
